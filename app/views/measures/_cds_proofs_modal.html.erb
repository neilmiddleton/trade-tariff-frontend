<div data-popup='<%= measure.destination %>-<%= measure.id %>-cds-proofs'>
  <% measure.cds_proofs_of_origin(rules_of_origin_schemes).each do |scheme| %>
    <article>
      <h2 class="govuk-heading-m">
        <%= scheme.title %>
      </h2>

      <%= render 'rules_of_origin/cds_proof_info', scheme: %>
    </article>
  <% end %>
</div>
